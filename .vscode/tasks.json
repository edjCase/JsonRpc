{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build Client Sample",
			"command": "dotnet",
			"type": "process",
			"args": [
				"build",
				"${workspaceFolder}/test/EdjCase.JsonRpc.Router.Sample/EdjCase.JsonRpc.Router.Sample.csproj",
				"/property:GenerateFullPaths=true"
			],
			"problemMatcher": "$msCompile"
		},
		{
			"label": "Build Router Sample",
			"command": "dotnet",
			"type": "process",
			"args": [
				"build",
				"${workspaceFolder}/test/EdjCase.JsonRpc.Router.Sample/EdjCase.JsonRpc.Router.Sample.csproj",
				"/property:GenerateFullPaths=true"
			],
			"problemMatcher": "$msCompile"
		},
		{
			"label": "Build All",
			"command": "dotnet",
			"type": "process",
			"args": [
				"build",
				"${workspaceFolder}/EdjCase.JsonRpc.sln",
				"/property:GenerateFullPaths=true"
			],
			"problemMatcher": "$msCompile",
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"label": "Test All",
			"command": "dotnet",
			"type": "process",
			"args": [
				"test",
				"${workspaceFolder}/test/EdjCase.JsonRpc.Router.Tests"
			],
			"problemMatcher": "$msCompile",
			"group": {
				"kind": "test",
				"isDefault": true
			}
		}
	]
}