{
	"webroot": "wwwroot",

	"dependencies": {
		"Microsoft.AspNetCore.Server.Kestrel": "1.0.0",
			"Microsoft.AspNetCore.Server.IISIntegration": "1.0.0",
			"Microsoft.AspNetCore.Authorization": "1.0.0",
			"EdjCase.BasicAuth": "1.0.0",
		"EdjCase.JsonRpc.Router": {
			"target": "project"
		}
	},
	
	"frameworks": {
		"net451": { },
		"netcoreapp1.0": {
			"dependencies": {
				"Microsoft.NETCore.App": {
					"version": "1.0.0",
					"type": "platform"
				}
			},
			"imports": [
				"dotnet52",
				"dnxcore50",
				"portable-net45+win8"
			]
		}
	},

	"buildOptions": {
		"emitEntryPoint": true,
		"preserveCompilationContext": true
	},

	"runtimeOptions": {
		"gcServer": true
	}
}
